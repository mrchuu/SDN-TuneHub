{
    "version": 2,
    "builds": [{
        "src": "./index.js",
        "use": "@vercel/node"
    }],
    "routes": [
        {
            "src": "/api/songs",
            "dest": "/Server/routes/song.js"
        },
        {
            "src": "/(.*)",
            "dest": "/Server/$1"
        }
    ]
}